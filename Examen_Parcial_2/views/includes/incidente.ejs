<div class="row" id="lista_incidentes">
    <%for (let incidente of incidentes){%>
        <div class="row">
            <div class="col s12 m6">
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                <span class="card-title"><%=incidente.nombreLugar%></span>
                <h5><%=incidente.nombreTipo%></h5>
                </div>
                <div class="card-action">
                <a href=""><%=incidente.fechayhora%></a>
                </div>
            </div>
            </div>
        </div>
    <%}%>
</div>